    "description": "MusicUserBot Untuk Memutar Lagu Dan Video Di Obrolan Suara Telegram.",
    "logo": "https://telegra.ph/file/613f681a511feb6d1b186.jpg",
    "keywords": [
        "Telegram",
        "MusicPlayer",
        "VideoPlayer"
    ],
    "repository": "https://github.com/Zaaen/MusicUserbot",
    "env": {
